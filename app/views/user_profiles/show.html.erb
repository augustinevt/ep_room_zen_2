<%= image_tag(@user_profile.avatar) %>
<h1>Hello <%= @user_profile.first_name %></h1>

<%= render 'images/form', object: @user_profile%>

<% @user_profile.images.each do |image| %>
  <%= image_tag(image.body) %>
  <h2><%= image.caption %></h2>
<%end%>

<h1>AJAX!</h1>

<% @user_profile.compatibilities.each do |c| %>

  <h1><%= c.question.title %>: <%= c.response%></h1>

<% end %>

<% @user_profile.compatibilities.each do |c| %>
  <%= c.question.title %>
  <%= render 'compatibilities/form', q: c.question, c: c  %>
<%end%>
