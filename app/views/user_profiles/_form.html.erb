<%= form_for UserProfile.new do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :avatar %>
  <%= f.file_field :avatar %>

  <%= f.submit %>

<%end%>
